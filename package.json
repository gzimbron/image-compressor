{
	"name": "imgpress",
	"version": "1.2.0",
	"description": "Compresor y redimensionador de imágenes con opciones de calidad y tamaño",
	"bin": {
		"imgpress": "bin/index.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsup src/index.ts --format cjs,esm --dts --clean"
	},
	"keywords": [
		"image",
		"compress",
		"resize"
	],
	"author": "Gustavo Zimbrón",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gzimbron/image-compressor.git"
	},
	"license": "ISC",
	"engines": {
		"node": ">=14.8.0"
	},
	"dependencies": {
		"commander": "^11.0.0",
		"sharp": "^0.32.1"
	},
	"devDependencies": {
		"@types/node": "20.14.10",
		"tsup": "8.1.0",
		"typescript": "5.5.3"
	}
}
